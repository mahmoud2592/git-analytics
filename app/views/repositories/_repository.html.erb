<div id="<%= dom_id repository %>">
  <p>
    <strong>Name:</strong>
    <%= repository.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= repository.description %>
  </p>

  <p>
    <strong>Programming language:</strong>
    <%= repository.programming_language %>
  </p>

  <p>
    <strong>License:</strong>
    <%= repository.license %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= repository.url %>
  </p>

  <p>
    <strong>Default branch:</strong>
    <%= repository.default_branch %>
  </p>

  <p>
    <strong>Stars count:</strong>
    <%= repository.stars_count %>
  </p>

  <p>
    <strong>Forks count:</strong>
    <%= repository.forks_count %>
  </p>

  <p>
    <strong>Watchers count:</strong>
    <%= repository.watchers_count %>
  </p>

  <p>
    <strong>Issues count:</strong>
    <%= repository.issues_count %>
  </p>

  <p>
    <strong>Pull requests count:</strong>
    <%= repository.pull_requests_count %>
  </p>

  <p>
    <strong>Commit count:</strong>
    <%= repository.commit_count %>
  </p>

</div>
